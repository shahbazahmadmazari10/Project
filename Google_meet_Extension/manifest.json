{
  "name": "GoogleMeet(SAM)",
  "description": "GoogleMeet Extension at first (disables mic & camera) then auto join the meeting !",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "hello.html",
    "default_icon": {
      "16": "/icons/mazari_16.png",
      "32": "/icons/mazari_32.png",
      "48": "/icons/mazari_48.png",
      "128": "/icons/mazari_128.png"
  }
},
"icons": {
  "16": "/icons/mazari_16.png",
  "32": "/icons/mazari_16.png",
  "48": "/icons/mazari_16.png",
  "128": "/icons/mazari_16.png"
  },
   "content_scripts":[ 
        {
      "matches":["https://meet.google.com/*"],    
      "js":["content.js"],
      "run_at":"document_end"
      }
     
   ] 
}
